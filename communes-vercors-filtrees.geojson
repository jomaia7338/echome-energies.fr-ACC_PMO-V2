const fs = require('fs');
const data = JSON.parse(fs.readFileSync('communes-france.geojson'));
const coeurVercors = [
  "Lans-en-Vercors",
  "Autrans-MÃ©audre-en-Vercors",
  "Saint-Nizier-du-Moucherotte",
  "Engins",
  "Villard-de-Lans"
];

const filtered = {
  type: "FeatureCollection",
  features: data.features.filter(f =>
    coeurVercors.includes(f.properties.nom)
  )
};

fs.writeFileSync("communes-vercors-filtrees.geojson", JSON.stringify(filtered));